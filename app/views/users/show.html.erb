<div class="container">
  <h1>My Blocmarks</h1>

  <div class="bookmarks">
    <div class="row">
      <h3>My Bookmarks</h3>
      <% @bookmarks.each do |bookmark| %>
        <div class="col-md-4 panel panel-default panel-body">
          <h5><%= link_to bookmark.url %></h5>
        </div>
      <% end %>
    </div>

  <div class="likes">
    <div class="row">
      <h3>My Likes</h3>
      <% @likes.each do |like| %>
        <div class="col-md-4 panel panel-default panel-body">
          <h5><%= link_to like.bookmark.url %></h5>
        </div>
      <% end %>
    </div>
  </div>
